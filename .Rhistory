nrow(cars)
nrow(train_cars)+nrow(test_cars)
train_cars<-select(train_cars, -Model, -Model.Index)
train_mat<-  model.matrix(model.frame(MPG~.,data=train_cars),data=train_cars)[,-1]
test_cars<-select(test_cars, -Model, -Model.Index)
